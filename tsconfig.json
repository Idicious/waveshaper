{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "module": "commonjs",
    "target": "es6",
    "allowJs": false,
    "allowUnreachableCode": false,
    "lib": ["es5", "es6", "dom"],
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": false,
    "noUnusedLocals": true,
    "declaration": true,
    "declarationDir": "./typings",
    "alwaysStrict": false,
    "noImplicitUseStrict": true
  }
}